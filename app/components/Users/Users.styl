@import "nib"
@import "rupture"

:local(.container) {
  display flex
  flex-flow row wrap
}

:local(.navigation) {
  display flex
  flex-flow row wrap
}

:local(.navigationItem) {
  padding 0.75rem
  text-decoration none
  color #222
  font-size 1.5rem
  font-weight 400
}

:local(.navigationList) {
  display flex
  flex-flow row wrap
  padding 0
  margin 0
  list-style none
}

:local(.navigationListItem) {
  display flex
  flex-flow row wrap
}
